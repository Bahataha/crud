{
    "name": "duke/crud-generator",
    "license": "MIT",
    "description": "Laravel CRUD Generator",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Bakytzhan Azimbay",
            "email": "fifty.tenge@gmail.com",
            "homepage": "http://www.azimbay.kz"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=5.6.4|>=7.0",
        "phpoffice/phpspreadsheet": "^1.6",
        "illuminate/support": "^5.3|^6.0",
        "laravelcollective/html": "^5.3|^6.0",
        "opis/closure": "^3.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.6",
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.3",
        "orchestra/database": "^4.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Duke\\CrudGenerator\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Duke\\CrudGenerator\\CrudGeneratorServiceProvider"
            ],
            "aliases": {
                "Excel": "Duke\\CrudGenerator\\Facades\\Excel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
